{
  "name": "ElasticKubeUI",
  "version": "1.0.0",
  "description": "ElasticKube UI",
  "watch": {
    "clean-dependencies": {
      "patterns": [
        "."
      ],
      "ignore": "app/*",
      "extensions": "json"
    }
  },
  "scripts": {
    "start": "parallelshell 'npm run build' 'npm-watch'",
    "clean-dependencies": "npm install && npm prune",
    "build": "webpack --progress --colors --devtool=eval --watch",
    "build:production": "NODE_ENV=production webpack -p --progress --colors --bail"
  },
  "dependencies": {
    "angular": "~1.4.9",
    "angular-animate": "~1.4.9",
    "angular-aria": "~1.4.9",
    "angular-cookies": "~1.4.9",
    "angular-material": "^1.0.4",
    "angular-messages": "~1.4.9",
    "angular-password": "^1.0.1",
    "angular-ui-grid": "^3.1.1",
    "angular-ui-router": "^0.2.18",
    "flux": "^2.1.1",
    "jquery": "~2.2.0",
    "lodash": "~4.2.1",
    "material-design-icons": "~2.1.3",
    "moment": "~2.11.2",
    "normalize.css": "~3.0.3"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.2",
    "babel-core": "^6.4.5",
    "babel-eslint": "^4.1.8",
    "babel-loader": "^6.2.2",
    "babel-preset-es2015": "^6.3.13",
    "css-loader": "^0.23.1",
    "eslint": "^1.10.3",
    "eslint-loader": "^1.2.1",
    "expose-loader": "^0.7.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "html-loader": "^0.4.0",
    "html-webpack-plugin": "^2.8.1",
    "img-loader": "^1.2.2",
    "json-loader": "^0.5.4",
    "less": "^2.6.0",
    "less-loader": "^2.2.2",
    "ng-annotate-loader": "^0.1.0",
    "ngtemplate-loader": "^1.3.1",
    "npm-watch": "^0.1.1",
    "parallelshell": "^2.0.0",
    "postcss-loader": "^0.8.0",
    "roboto-fontface": "^0.4.2",
    "style-loader": "^0.13.0",
    "svgo": "^0.6.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.12"
  }
}
